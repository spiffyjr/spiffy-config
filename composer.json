{
    "name": "spiffy/spiffy-config",
    "description": "ZF2 module that provides route annotations directly in controllers.",
    "homepage": "http://www.github.com/spiffyjr/spiffy-config.git",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.3.3",
        "doctrine/common": "~2.2",
        "symfony/finder": "~2.2",
        "zendframework/zend-cache": "~2.2",
        "zendframework/zend-code": "~2.2",
        "zendframework/zend-console": "~2.2",
        "zendframework/zend-log": "~2.2",
        "zendframework/zend-mvc": "~2.2",
        "zendframework/zend-serializer": "~2.2",
        "zendframework/zend-version": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*",
        "satooshi/php-coveralls": "dev-master",
        "spiffy/spiffy-test": "dev-master",
        "zendframework/zend-test": "2.*"
    },
    "autoload": {
        "psr-0": {
            "SpiffyConfig": "src/",
            "SpiffyConfigTest": "test/"
        }
    },
    "extra": {
        "branch-alias": {
	    "dev-develop": "1.x-dev"
	}
    }
}
